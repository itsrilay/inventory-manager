<div class="page-header">
  <h1 class="page-title"><%= title %></h1>
  <a class="btn" href="/categories/create">Add Category</a>
</div>
<main>
  <div class="item-list">
    <div class="item category">
      <h3>Name</h3>
    </div>
    <% categories.forEach((category) => { %> <%- include('category', {category:
    category}) %> <% }) %>
  </div>
</main>
