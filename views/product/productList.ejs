<div class="page-header">
  <h1 class="page-title"><%= title %></h1>
  <a class="btn" href="/products/create">Add Product</a>
</div>
<main class="item-container">
  <div class="item-list">
    <div class="item product">
      <h3>Name</h3>
      <h3>Price</h3>
      <h3>Quantity</h3>
      <h3>Availability</h3>
    </div>
    <% products.forEach((product) => { %> <%- include('product',
    {product:product}) %> <% }) %>
  </div>
</main>
